inder\n| unit_pref               = US\n| area_total_sq_mi        = 312.21\n| area_land_sq_mi         = 309.13\n| area_water_sq_mi        = 3.07\n| area_metro_sq_mi        =\n| area_rank               = [[List of counties in Indiana|65th]] largest county in Indiana\n| elevation_ft            = 712\n| population_total        = 23,363\n| population_as_of        = [[2010 United States Census|'''2010''']]\n| population_density_sq_mi= 75.6\n| population_est          = 22,935\n| pop_est_as_of           = 2018\n| population_rank         = {{nowrap|[[List of counties in Indiana|78th]] largest county in Indiana<br>1,679th largest county in U.S.<ref>{{cite web|url=http://www.stats.indiana.edu/uspr/a/us_profile_frame.html|title=USA Counties in Profile|accessdate=29 June 2014|publisher=STATS Indiana}}</ref>}}\n| timezone                = [[Central Standard Time|Central]]\n|utc_offset               = \u22126\n| timezone_DST            = [[Central Daylight Time|Central]]\n|utc_offset_DST           = \u22125\n| postal_code_type        = [[ZIP Code]]s\n| postal_code             = 46348, 46366, 46374, 46511, 46531-32, 46534, 46574, 46960\n| area_code_type          = [[North American Numbering Plan|Area code]]\n| area_code               = [[Area code 574|574]]\n| geocode                 =\n| iso_code                =\n| blank_name_sec1         = Congressional district\n| blank_info_sec1         = [[Indiana's 2nd congressional district|2nd]]\n| blank1_name_sec1        = [[Indiana Senate]] district\n| blank1_info_sec1        = 5th\n| blank2_name_sec1        = [[Indiana House of Representatives]] district\n| blank2_info_sec1        = 17th\n| blank3_name_sec1        = [[Federal Information Processing Standard|FIPS code]]\n| blank3_info_sec1        = 18-149\n| blank4_name_sec1        = [[Geographic Names Information System|GNIS]] feature ID\n| blank4_info_sec1        = 0450389\n| blank_name_sec2        = [[U.S. Routes]]\n| blank_info_sec2        = [[File:US 30.svg|27px|link=U.S. Route 30 i