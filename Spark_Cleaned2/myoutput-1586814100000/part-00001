ton]]\n|team3=CAM|name_CAM=[[Cambridge United F.C.|Cambridge United]]\n|win_BRR=2|draw_BRR=0|loss_BRR=0|gf_BRR=5|ga_BRR=0\n|win_BRT=1|draw_BRT=0|loss_BRT=1|gf_BRT=4|ga_BRT=3\n|win_CAM=0|draw_CAM=0|loss_CAM=2|gf_CAM=1|ga_CAM=7\n\n|col_A=#ccffcc|text_A=\n|result1=A|result2=A\n|update=complete|source=\n}}\n{{OneLegStart|date=y|team-width=15em}}\n{{OneLegResult|date=26 Sep|Cambridge United||1\u20134|Brighton & Hove Albion}}\n{{OneLegResult|date=17 Oct|Brighton & Hove Albion||0\u20132|Bristol Rovers}}\n{{OneLegResult|date=7 Nov|Bristol Rovers||3\u20130|Cambridge United}}\n{{end}}\n{{col-end}}\n{{col-begin}}\n{{col-2}}\n{{#invoke:sports table|main|style=WDL\n|res_col_header=Q\n|show_positions=n\n|ptsfirst=y\n|sortable_table=y\n|title=Group 7\n\n|team1=OXF|name_OXF=[[Oxford United F.C.|Oxford United]]\n|team2=BRI|name_BRI=[[Bristol City F.C.|Bristol City]]\n|team3=BAR|name_BAR=[[Barnet F.C.|Barnet]]\n|win_OXF=2|draw_OXF=0|loss_OXF=0|gf_OXF=6|ga_OXF=2\n|win_BRI=1|draw_BRI=0|loss_BRI=1|gf_BRI=2|ga_BRI=3\n|win_BAR=0|draw_BAR=0|loss_BAR=2|gf_BAR=2|ga_BAR=5\n\n|col_A=#ccffcc|text_A=\n|result1=A|result2=A\n|update=complete|source=\n}}\n{{OneLegStart|date=y|team-width=15em}}\n{{OneLegResult|date=26 Sep|Oxford United||3\u20130|Bristol City}}\n{{OneLegResult|date=17 Oct|Bristol City||2\u20130|Barnet}}\n{{OneLegResult|date=7 Nov|Barnet||2\u20133|Oxford United}}\n{{end}}\n{{col-2}}\n{{#invoke:sports table|main|style=WDL\n|res_col_header=Q\n|show_positions=n\n|ptsfirst=y\n|sortable_table=y\n|title=Group 8\n\n|team1=SWI|name_SWI=[[Swindon Town F.C.|Swindon Town]]\n|team2=COL|name_COL=[[Colchester United F.C.|Colchester United]]\n|team3=TOR|name_TOR=[[Torquay United F.C.|Torquay United]]\n|win_SWI=1|draw_SWI=1|loss_SWI=0|gf_SWI=3|ga_SWI=1\n|win_COL=1|draw_COL=0|loss_COL=1|gf_COL=5|ga_COL=4\n|win_TOR=0|draw_TOR=1|loss_TOR=1|gf_TOR=3|ga_TOR=6\n\n|col_A=#ccffcc|text_A=\n|result1=A|result2=A\n|update=complete|source=\n}}\n{{OneLegStart|date=y|team-width=15em}}\n{{OneLegResult|date=26 Sep|Co